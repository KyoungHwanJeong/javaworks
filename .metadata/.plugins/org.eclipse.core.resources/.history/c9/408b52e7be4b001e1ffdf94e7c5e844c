package data;



public class Var2Info extends Var2 {

	Var2Info varInfo = new Var2Info();

	public String showVar2Info(double inchTocm) {
		return ("1 inch는 " +  inchTocm + "입니다.");
	}
}

class Var2 {



	
	public static void main(String[] args) {
		// 1 inch는 2.54 cm입니다.
		// 실수형 자료형은 double, float를 사용한다.
		
		double inchTocm = 2.54;
		// float inchTocom = 2.54f;	// float형은 값 뒤에 f나 F를 붙인다.
		
		
//		System.out.println("1 inch는 " + inchTocm + "입니다.");
		System.out.println(Var2Info.showVar2Info(inchTocm));
		

		
		
	}
	
}
