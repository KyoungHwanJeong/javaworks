package overriding.shop;

public class MainClass2 {

	public static void main(String[] args) {
		
		// 자동 형변환
		// 부모형으로 객체 생성 - 자동 형 변환
		HeadShop shop_1 = new Shop1();	// HeadShop > Shop
		shop_1.비빔밥();
		
		System.out.println("====================================");
		
		HeadShop shop_2 = new shop2();
