package kr.or.iei.model;

//회원 클래스(자료형) 정의
public class User {

	// 필드(속성)
	private String id;
	private String pwd;
	private String name;
	
	//set(), get()함수
	public void setId(String id) {
		this.id= id;
	}
	public void setPwd(String pwd) {
		this.pwd= pwd;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public String getPwd() {
		return pwd;
	}
	public String getName() {
		return name;
	}
	
	
}
