package classes_2;

public class Calculator_2 {

	public int add(int num1,int num2) {
		return num1+num2;
	}
	public int sub(int num1,int num2) {
		return num1-num2;
	}
	public int mul(int num1,int num2) {
		return num1*num2;
	}
	public double div(int num1,int num2) {
		return (double)(num1/num2);
	}
}
