package operator;

public class OperatorEx2 {

	public static void main(String[] args) {

		// 비교 연산자
		// 결과는 boolean 자료임
		
		System.out.println(4 < 5);	// true
		System.out.println(4 > 5);	// false
		System.out.println(4 == 5);	// false
		System.out.println(4 != 5);	// true
		
		
		// 변수를 사용해서 비교 연산하기
		
		int n1= 4, n2= 6;
		System.out.println(n1 < n2);
		System.out.println(n1 > n2);
		System.out.println(n1 == n2);
		System.out.println(n1 != n2);
	}

}
