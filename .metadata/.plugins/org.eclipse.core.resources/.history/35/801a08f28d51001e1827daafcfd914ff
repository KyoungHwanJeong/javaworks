package kr.or.iei_2.model;

public class User {

	private String id;
	private int pwd;
	private String name;
	
	public void setId(String id) {
		this.id=id;
	}
	public void setPwd(int pwd) {
		this.pwd=pwd;
	}
	public void setName(String name) {
		this.name=name;
	}
	public String getId() {
		return id;
	}
	public int getPwd() {
		return pwd;
	}
	public String getName() {
		return name;
	}
	public void User(String id, int pwd, String name) {
		this.id=id;
		this.pwd=pwd;
		this.name=name;
	}
	
}
