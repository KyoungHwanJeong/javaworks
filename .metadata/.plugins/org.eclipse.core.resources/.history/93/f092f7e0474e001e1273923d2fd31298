package ifexample;

public class MaxNumberCal {


	
	public static int maxNumberCal(int n1, int n2) {

		
		// 연산 1. if ~else구문
		if(n1 >= n2) {		// n1 > n2 || n1 == n2
			return n1;
		}else {				// n1 < n2
			return n2;
		}
		
		// 연산 2. 조건 연산자
//		int maxNum = (n1 >= n2) ? n1 : n2;
		
		
		// 출력
	}
}
