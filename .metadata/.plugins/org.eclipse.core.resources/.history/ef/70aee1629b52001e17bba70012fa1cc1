package kr.or.iei.model;

public class User {

	private int id;
	private int pwd;
	private String name;
	
	User(){
		
	}
	
	public void setId(int id) {
		this.id=id;
	}
	public void setPwd(int pwd) {
		this.pwd=pwd;
	}
	public void setName(String name) {
		this.name=name;
	}
	
	public void showUserInfo() {
		System.out.print("");
	}
	
}
