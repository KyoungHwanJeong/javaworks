package overriding.shop;

public class MainClass {

	public static void main(String[] args) {

		// 매장 객체(인스턴스) 생성
		Shop1 shop_1 = new Shop1();
		shop_1.된장찌게();
		shop_1.김치찌게();
		shop_1.비빔밥();

		System.out.println("================================");
		
		Shop2 shop_2 =new Shop2();
		shop_2.된장찌게();
		shop_2.김치찌게();
		shop_2.비빔밥();
		
	}

}
