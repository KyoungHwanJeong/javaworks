package thissamples;

public class ThisTest {

	public static void main(String[] args) {

		/*
		 * BirthDay 클래스 함수를 호출해서
		 * year의 값 2023을 넣고,
		 * main함수에서 출력한 값 2023이 들어간 주소값과
		 * BirthDay 클래스의 함수에서 가리키는 주소값을 BirthDay 클래스에서 출력하기.
		 */
		
		// 생성된 인스턴스 - dDay와 클래스의 this 속성과 주소가 같다.
		BirthDay dDay = new BirthDay();
		dDay.setYear(2023);
		
		System.out.println(dDay);
		dDay.printThis();
	}

}
