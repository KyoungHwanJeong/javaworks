package forexample;

public class NestedFor2_2 {

	public static void main(String[] args) {

		/*
		 * num = 22까지 숫자를 5열에 배치하는 프로그램
		*/
		
		
		for(int i=0; i<5;i++) {
			for(int j=0; j<5; j++) {
				if(5*i+j>22)
					break;
				System.out.printf("%5d", 5*i+j);
			}
			System.out.println();
		}
		
	}

}
